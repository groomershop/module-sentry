{
    "name": "mygento/module-sentry",
    "type": "magento2-module",
    "version": "2.4.4",
    "license": "OSL-3.0",
    "homepage": "https://github.com/mygento/module-sentry",
    "description": "Magento 2 Sentry Logger",
    "authors": [
        {
            "name": "Nikita Tarasov",
            "email": "nikita@mygento.com",
            "homepage": "http://www.mygento.com/",
            "role": "Developer"
        },
        {
            "name": "Mygento",
            "email": "connect@mygento.com",
            "homepage": "https://www.mygento.com/"
        }
    ],
    "keywords": [
        "magento2",
        "mygento",
        "sentry"
    ],
    "require": {
        "magento/framework": "103.*",
        "magento/module-config": "101.*",
        "sentry/sdk": "^4"
    },
    "require-dev": {
        "mygento/coding-standard": ">=2.14.0 <2.14.5"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Mygento\\Sentry\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true,
            "magento/composer-dependency-version-audit-plugin": false
        }
    }
}
